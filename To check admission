#include<stdio.h>
int main()
{
    float Math;
    float Phy;
    float Chem;
    float MathPhy;
    float Total;
    printf("Enter the marks of Maths :");
    scanf("%f",&Math);
    printf("Enter the marks of Physics :");
    scanf("%f",&Phy);
    printf("Enter the marks of Chemistry :");
    scanf("%f",&Chem);
    MathPhy = Math + Phy;
    Total = Math + Phy + Chem;
    
    if (Math>=65 && Phy>=55 && Chem>=50 && MathPhy>140 && Total>=190)
    {
        printf("You are Eligible for Admission in this College");
    }
    else
    {
        printf("Sorry!!, You are not Eligible for Admission");
    }
    return 0;
}
